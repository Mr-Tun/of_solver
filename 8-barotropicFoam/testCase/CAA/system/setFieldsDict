FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
);

regions
(
    sphereToCell
    {
        centre  (10 5 0);
        radius  0.2;
        fieldValues
        (
            volScalarFieldValue p 101900
        );
    }
);


