FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       rhoFo;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           0.07;

deltaT            0.0001;

writeControl      timeStep;

writeInterval     20;

purgeWrite        0;

writeFormat       binary;

writePrecision    10;

writeCompression  off;

timeFormat        general;

timePrecision     6;

runTimeModifiable true;


