FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 14.855;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 14.855;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 14.855;
        value           uniform 14.855;
    }

    upperWall
    {
    	type            zeroGradient;
        //type            fixedValue;
        //type            epsilonWallFunction;
        //value           uniform 0.0001;
    }

    lowerWall
    {
    	type            zeroGradient;
        //type            fixedValue;
        //type            epsilonWallFunction;
        //value           uniform 0.0001;
    }

    frontAndBack
    {
        type            empty;
    }
}

